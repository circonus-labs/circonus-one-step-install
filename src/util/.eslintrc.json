{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "root": true,
    "rules": {
        "callback-return": 2,
        "global-require": 2,
        "handle-callback-err": [2, "^(err|error)$"],
        "no-mixed-requires": [2, {"grouping": true}],
        "no-new-require": 2,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "arrow-parens": [2, "always"],
        "arrow-spacing": [2, {"before": true, "after": true}],
        "constructor-super": 2,
        "generator-star-spacing": [2, { "before": false, "after": true }],
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-this-before-super": 2,
        "no-var": 2,
        "object-shorthand": [2, "always"],
        "prefer-arrow-callback": 2,
        "prefer-const": 2,
        "prefer-template": 2,
        "require-yield": 2,
        "init-declarations": [2, "always"],
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-shadow": 2,
        "no-undef": 2,
        "no-undefined": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "block-scoped-var": 2,
        "accessor-pairs": [2, {"getWithoutSet": false, "setWithoutGet": false}],
        "consistent-return": 2,
        "curly": 2,
        "default-case": 2,
        "dot-location": [2, "object"],
        "dot-notation": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": [ 2, { "allowLoop": false, "allowSwitch": false }],
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-magic-numbers": [ 2, { "ignoreArrayIndexes": true }],
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-param-reassign": [2, {"props": true}],
        "no-process-env": 2,
        "no-proto": 2,
        "no-redeclare": [2, {"builtinGlobals": true}],
        "no-return-assign": [2, "always"],
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-void": 2,
        "no-warning-comments": [1, { "terms": ["TODO", "FIXME", "BUG", "TBD"], "location": "anywhere"}],
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 2,
        "wrap-iife": [2, "outside"],
        "comma-dangle": [2, "never"],
        "no-cond-assign": [2, "except-parens"],
        "no-console": 0,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": [2, "both"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": [2, {"requireReturn": false}],
        "valid-typeof": 2,
        "strict": [
            2,
            "global"
        ],
        "brace-style": [
            2,
            "stroustrup",
            {
                "allowSingleLine": true
            }
        ],
        "wrap-regex": 2,
        "space-unary-ops": [
            1,
            {
                "words": true,
                "nonwords": false
            }
        ],
        "array-bracket-spacing": [2, "always"],
        "space-infix-ops": [
            2,
            {
                "int32Hint": true
            }
        ],
        "keyword-spacing": [ 2, { "before": true, "after": true }],
        "space-before-function-paren": [
            2,
            "never"
        ],
        "block-spacing": [2, "always"],
        "space-before-blocks": [
            2,
            "always"
        ],
        "sort-vars": [
            2,
            {
                "ignoreCase": true
            }
        ],
        "semi-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1,
                "VariableDeclarator": 1
            }
        ],
        "eol-last": 2,
        "consistent-this": [2, "self"],
        "comma-style": [2, "last"],
        "comma-spacing": [2, { "before": false, "after": true }],
        "camelcase": [2, {"properties": "always"}],
        "quotes": [
            2,
            "double",
            {
                "avoidEscape": true,
                "allowTemplateLiterals": true
            }
        ],
        "operator-assignment": [
            2,
            "always"
        ],
        "object-curly-spacing": [
            2,
            "always"
        ],
        "no-unneeded-ternary": 2,
        "no-trailing-spaces": 2,
        "no-spaced-func": 2,
        "no-new-object": 2,
        "no-nested-ternary": 2,
        "no-negated-condition": 2,
        "no-multiple-empty-lines": [2, { "max": 2 }],
        "no-mixed-spaces-and-tabs": 2,
        "no-lonely-if": 2,
        "no-array-constructor": 2,
        "newline-after-var": [2, "always"],
        "new-parens": 2,
        "new-cap": [
            2,
            {
                "newIsCap": true,
                "capIsNew": true
            }
        ],
        "max-nested-callbacks": [
            2,
            3
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    }
}

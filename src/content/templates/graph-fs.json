                    "color": "#657aa6",
                    "data_formula": null,
                    "derive": "gauge",
                    "hidden": false,
                    "legend_formula": null,
                    "metric_name": "fs`([^`]+)`df_used_percent",
                    "metric_type": "numeric",
                    "name": "% Used (bytes)",
                    "stack": null
                },
                {
                    "alpha": "0.3",
                    "axis": "l",
                    "check_id": null,
                    "color": "#4fa18e",
                    "data_formula": null,
                    "derive": "gauge",
                    "hidden": false,
                    "legend_formula": null,

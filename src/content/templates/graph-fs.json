    "variable_metrics": true,
    "filter": {
        "exclude": [
            "^/boot$",
            "^/dev$",
            "^/dev/shm$",
            "^/run/user",
            "^/sys/fs/cgroup$"
        ],
        "include": null
    },
    "graphs": [
